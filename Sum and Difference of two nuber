#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a=0,b=0;
    float c=0,d=0;
    scanf("%d%d",&a,&b);
    scanf("%f%f",&c,&d);
   /* sum=a+b;
    sub=a-b;
    s=a+b;
    su=a-b;*/
    printf("%d %d\n",a+b,a-b);
    printf("%.1f %.1f",c+d,c-d);
    
    return 0;
}
